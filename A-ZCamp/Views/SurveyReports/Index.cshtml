@model A_ZCamp.Models.REPORTVIEWMODEL
@{
    ViewBag.Title = "Reporting";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<h2>Reporting</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Counter</th>
            <th>Survey Name</th>
            <th>Question</th>
            <th>Response</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var x in Model.reportData)
        {
            <tr>
                <th scope="row">1</th>
                <td>@x.Survey</td>
                <td>@x.Question</td>
                <td>@x.Response</td>
            </tr> 
        }
    </tbody>
</table>
